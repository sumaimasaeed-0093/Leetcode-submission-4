#include <stdio.h>

int main() {
    int n;
    printf("Enter n:");
    scanf("%d", &n);

    for (int i = 0; i < n; i++) {
        int length, width, height;
        printf("Enter length:");
        scanf("%d", &length);
        printf("Enter width:");
        scanf("%d", &width);

        if (height < 41) {
            int volume = length * width * height;
            printf("%d\n", volume);
        }
    }

    return 0;
}
