#include <stdio.h>

struct Student {
    int age;
    char first_name[51];
    char last_name[51];
    int standard;
};

int main() {
    struct Student s;
    printf("Enter the age of student: ");
    scanf("%d", &s.age);
    printf("Enter the first name of student: ");
    scanf("%s", s.first_name);
    printf("Enter the last name of student: ");
    scanf("%s", s.last_name);
    printf("Enter the standard of student: ");
    scanf("%d", &s.standard);

    printf("%d %s %s %d", s.age, s.first_name, s.last_name, s.standard);

    return 0;
}
